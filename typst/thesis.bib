@article{Howes1991,
  title = {Pharmacokinetic Comparison of a Combination Tablet of Enalapril and Hydrochlorothiazide with Enalapril and Hydrochlorothiazide Tablets Administered  Together and Separately.},
  author = {Howes, L. G. and Conway, E. L. and Phillips, P. A. and Broadbear, J. and Drummer, O. H. and Louis, W. J.},
