@article{Niemeyer1983,
  title = {Pharmacokinetics of Hydrochlorothiazide in Relation to Renal Function.},
  author = {Niemeyer, C. and Hasenfuss, G. and Wais, U. and Knauf, H. and {Sch{\"a}fer-Korting}, M. and Mutschler, E.},
